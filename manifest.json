{
  "manifest_version": 2,
  "name": "TestExtension",
  "version": "2023.1.0",
  "background": {
    "scripts": ["baseMessengers.js","testCommon.js", "testBackground.js"]
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "storage",
    "tabs",
    "webNavigation",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["baseMessengers.js",  "testCommon.js", "testContent.js"]
    }
  ],
  "web_accessible_resources": [
  ]
}